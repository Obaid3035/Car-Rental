<div class="container-fluid">
  <div class="col-sm-12">
    <div class="container" style="background-color: white; border-radius: 25px;">
      <table class="table">
          <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Created At</th>
            <th>Updated At</th>
          </tr>
        <tbody>
          <% @brand.each do |brand| %>
            <tr>
              <th><%=brand.id%></th>
              <td><%=brand.name%></td>
              <td><%=brand.created_at%></td>
              <td><%=brand.updated_at%></td>
              <td><%= link_to 'Edit', edit_admins_brand_path(brand), remote: true, data: { controller: 'open-form-modal', action: 'ajax:success->open-form-modal#openModal' } %></td>
              <td><%= link_to 'Delete', admins_brand_path(brand),
                                        method: :delete,
                                        data: { confirm: 'Are you sure' } %> </td>
            </tr>
          <% end %>
        </tbody>
      </table>
      </div>
  </div>
</div>